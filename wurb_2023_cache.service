[Unit]
Description=CloudedBats WURB-2023 Cache files management.  
After=network.target

[Service]
Restart=always
TimeoutStopSec=10s
User=pi
WorkingDirectory=/home/pi/cloudedbats_wurb_2023
ExecStart=/home/pi/cloudedbats_wurb_2023/venv/bin/python3 /home/pi/cloudedbats_wurb_2023/wurb_cache_main.py

[Install]
WantedBy=multi-user.target